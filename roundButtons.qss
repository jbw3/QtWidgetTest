QPushButton
{
    background-color: rgb(180, 180, 180);
    border-style: solid;
    border-width:1px;
    border-radius:10px;
    border-color: rgb(180, 180, 180);
    max-width:20px;
    max-height:20px;
    min-width:20px;
    min-height:20px;
}

QPushButton:pressed
{
    background-color: rgb(80, 80, 80);
    border-color: rgb(80, 80, 80);
}
